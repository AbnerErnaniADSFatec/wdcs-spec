{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://www.esensing.dpi.inpe.br/wdcs/list_cubes_response.json",
  "type": "object",
  "required": [
    "wdcs_version",
    "wdcs_provider",
    "list_cubes"
  ],
  "properties": {
    "wdcs_version": {
      "type": "string",
      "example": "0.1.2"
    },
    "wdcs_provider": {
      "type": "string",
      "example": "INPE"
    },
    "list_cubes": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "string"
      },
      "example": ["S10mMEDIAN", "S10mSTACK"]
    }
  }
}